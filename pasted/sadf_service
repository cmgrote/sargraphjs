# default: on

service paste
{
  port = 444
  bind = 0.0.0.0
  port = 3000
  type = UNLISTED
  socket_type = stream
  wait = no
  user = paste
  server = /home/paste/pasted/sadfd.sh
  log_on_success += USERID PID HOST EXIT DURATION
  log_on_failure += USERID HOST ATTEMPT
  disable = no
  log_type = FILE /tmp/paste.xinetd.log
  passenv = REMOTE_HOST
}
